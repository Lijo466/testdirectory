version: '3.1'

services:
  db:
    image: mariadb
    container_name: mariadb_prd
    restart: always
    environment:
      MYSQL_ROOT_PASSWORD: user#1234
      MYSQL_DATABASE: test
      MYSQL_USER: testuser
      MYSQL_PASSWORD: test#1234
    ports:
      - "1793:3306"
    volumes:
      - ./data:/var/lib/mysql
